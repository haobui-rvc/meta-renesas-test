+++ b/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
@@ -430,6 +430,18 @@
 	};
 };
 
+&tmu1 {
+	status = "okay";
+};
+
+&tmu2 {
+	status = "okay";
+};
+
+&tmu3 {
+	status = "okay";
+};
+
 &du {
 	pinctrl-0 = <&du0_pins>;
 	pinctrl-names = "default";
