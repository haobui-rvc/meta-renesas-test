+			compatible = "renesas,r8a7742-mstp-clocks",
+						"renesas,cpg-mstp-clocks";
+			reg = <0 0xe615014c 0 4>, <0 0xe61501c4 0 4>;
+			clocks = <&mp_clk>, <&hp_clk>, <&zs_clk>, <&zs_clk>,
+			<&p_clk>, <&p_clk>, <&zx_clk>, <&zx_clk>, <&zx_clk>,
+			<&zx_clk>, <&zx_clk>;
