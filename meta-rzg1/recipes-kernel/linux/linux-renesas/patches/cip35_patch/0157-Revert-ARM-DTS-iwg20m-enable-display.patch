From bd5744f3475fe65d9dc3197f137b650425f2f138 Mon Sep 17 00:00:00 2001
From: thongsyho <thong.ho.px@rvc.renesas.com>
Date: Fri, 22 Jun 2018 14:29:45 +0700
Subject: [PATCH 157/628] Revert "ARM: DTS: iwg20m: enable display"

This reverts commit 57ce7847e334 ("ARM: DTS: iwg20m: enable display")

Signed-off-by: thongsyho <thong.ho.px@rvc.renesas.com>
---
 arch/arm/boot/dts/iwg20d-q7-dbcm-ca.dtsi | 40 --------------------------------
 1 file changed, 40 deletions(-)

diff --git a/arch/arm/boot/dts/iwg20d-q7-dbcm-ca.dtsi b/arch/arm/boot/dts/iwg20d-q7-dbcm-ca.dtsi
index 36fc539..b5b9efd 100644
--- a/arch/arm/boot/dts/iwg20d-q7-dbcm-ca.dtsi
+++ b/arch/arm/boot/dts/iwg20d-q7-dbcm-ca.dtsi
@@ -20,41 +20,6 @@
 		clock-frequency = <12000000>;
 	};
 
-	vcc_lvds: regulator@4 {
-		compatible = "regulator-fixed";
-
-		regulator-name = "panel vcc";
-		regulator-min-microvolt = <3300000>;
-		regulator-max-microvolt = <3300000>;
-		regulator-always-on;
-	};
-
-	x3_clk: x3-clock {
-		compatible = "fixed-clock";
-		#clock-cells = <0>;
-		clock-frequency = <148500000>;
-	};
-
-	panel_hannstar: panel@0 {
-		compatible = "hannstar,hsd070pww1", "simple-panel";
-		power-supply = <&vcc_lvds>;
-
-		ports {
-			#address-cells = <1>;
-			#size-cells = <0>;
-
-			port@0 {
-				#address-cells = <1>;
-				#size-cells = <0>;
-				reg = <0>;
-				panel_input: endpoint {
-					reg = <0>;
-					remote-endpoint = <&du_out_lvds0>;
-				};
-			};
-		};
-	};
-
 	hdmi-out {
 		compatible = "hdmi-connector";
 		type = "a";
@@ -108,11 +73,6 @@
 				remote-endpoint = <&adv7511_in>;
 			};
 		};
-		port@1 {
-			endpoint {
-				remote-endpoint = <&panel_input>;
-			};
-		};
 	};
 };
 
